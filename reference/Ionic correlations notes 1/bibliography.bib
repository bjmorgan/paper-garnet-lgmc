@article{MartonakEtAl_JChemPhys2002,
	Abstract = {We use a recently developed constant-pressure molecular dynamics method
	for nonperiodic systems to study pressure-induced structural transformations
	in medium-sized silicon nanocrystals, where the kinetics is experimentally
	known to be bulk rather than surface dominated, choosing Si705 as
	a representative example. Pressure is applied and tuned through a
	liquid described by a classical potential, while the nanocrystal
	is treated within a tight-binding scheme. Upon pressurization the
	nanocrystal undergoes a structural transformation which starts at
	the surface and gradually propagates into the bulk core. The high-pressure
	structure is disordered and metallic, with an x-ray diffraction pattern
	compatible with both the ideal b-tin and simple hexagonal structures.
	Strong similarities with a recently calculated high-pressure phase
	of bulk amorphous silicon are evident. Upon pressure release, the
	original diamond structure is not recovered and a high degree of
	disorder persists.},
	Author = {Marto\v{n}\'{a}k, R. and Colombo, L. and Molteni, C. and Parrinello, M.},
	File = {Martonak et al - Journal of Chemical Physics 2002.pdf:C\:\\Documents and Settings\\Ben\\My Documents\\Papers\\Nanocrystals\\Martonak et al - Journal of Chemical Physics 2002.pdf:PDF},
