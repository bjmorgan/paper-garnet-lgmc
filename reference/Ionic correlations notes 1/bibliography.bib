	Volume = {53},
	Year = {1996},
	Bdsk-Url-1 = {http://dx.doi.org/10.1103/PhysRevB.53.8262}}

@article{NeeleshwarEtAl_PhysRevB2005,
	Abstract = {Temperature dependences of the magnetic susceptibility $\chi$ and
