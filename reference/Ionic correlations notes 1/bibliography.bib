@article{LinEtAl_PhysRevB2009,
	Abstract = {The effect of atomic hydrogen adsorption on TiO2-terminated and SrO-terminated
	SrTiO3 (001) surfaces is studied using density functional theory
	calculations. Several adsorption coverages (1/12, 1/6, 1/3, 1/2,
	2/3, and 1 monolayer) are considered. It is found that the hydrogen
