@article{LiEtAl_ApplPhysLett2007,
	Abstract = {Size-dependent ground state energies and electronic structures of
	ZnO nanoclusters with wurtzite structures are investigated using
	density-functional theory calculations. After structural optimizations,
	the initial Zn--O double layers merge into single layers. Particularly,
	a phase transition from the four-coordinate wurtzite to the six-coordinate
