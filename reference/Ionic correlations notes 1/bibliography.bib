	Owner = {Ben},
	Pages = {4156--4161},
	Timestamp = {2008.05.08},
	Title = {Photophysics and Electron Dynamics in Dye-Sensitized Semiconductor Film Studied by Time-Resolved Mis-IR Spectroscopy},
