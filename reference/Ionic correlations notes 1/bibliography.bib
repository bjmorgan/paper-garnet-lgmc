	Pages = {13023--13032},
	Timestamp = {2007.03.28},
	Title = {Electronic and optical properties of three phases of titanium dixide: Rutile, anatase, and brookite},
	Volume = {51},
	Year = {1995}}

@article{MohamedEtAl_PhysRevB1988,
	Abstract = {Electron-energy-loss measurements on the stoichiometric TiO2(110)
	surface which include electronic excitations from both the O 2p valence
	states and the O 2s, Ti 3p, and Ti 3s core levels have been performed.
	The observed features have been empirically interpreted within the
	framework of the molecular-orbital description of an octahedral complex.
	The effects of surface defects created by Ar+-ion bombardment and
	by the evaporation of a thin Ti overlayer on the energy-loss results
	have also been studied and were found to cause marked changes in
	both the valence and core loss spectra. This sensitivity arises from
	changes in the screening between surface cations that occur when
	the oxygen-titanium ratio at the surface is varied.},
