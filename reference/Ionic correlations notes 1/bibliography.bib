@article{HeEtAl_SolStatComm2007,
	Abstract = {The electronic structure and ferromagnetism of V-doped rutile and
	anatase TiO2 without vacancy and with single oxygen vacancy are studied
	by means of first principle calculations based on the density-functional
	theory and pseudopotential method. It can be found that V-doped rutile
