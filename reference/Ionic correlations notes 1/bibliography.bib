@article{TielensEtAl_JElectroanalChem2005,
	Abstract = {The Li atom positions are calculated in LixTiO2 anatase, by means
	of periodic density functional theory calculations, followed by the
	predictions of the energy barriers for the mobility of Li atoms,
	done for the first time for a range of Li concentrations (x < 0.5).
