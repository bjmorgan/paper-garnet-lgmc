@article{DoubletEtAl_ChemMater2002,
	Abstract = {We present the ternary phases LixVPn4 (Pn ) P, As) as new materials
	for the negative electrode in lithium-ion batteries. Associated with
	a large variation of lithium content per formula unit (3 e x e 7.5
	for P and 3 e x e 11 for As), these materials show a higher specific
