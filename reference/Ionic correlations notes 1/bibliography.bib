@article{SheppardEtAl_JPhysChemB2006b,
	Abstract = {The present work reports isothermal gas/solid equilibration kinetics
	for Nb-doped TiO2 (0.65 atom %) at elevated temperatures (1073-1298
	K) within narrow ranges of oxygen activity spanning between 10-13
	Pa and 75 kPa. The equilibration kinetics were monitored using electrical
	conductivity measurements. The kinetic data were used to determine
	the chemical diffusion coefficient (Dchem). Dchem as a function of
	p(O2) exhibits a complex dependence, which is considered in terms
	of defect disorder and the related concentrations of electronic charge
	carriers. The activation energy of Dchem in the p(O2) range 10 Pa
	< p(O2) < 75 kPa varies in the range 88.0-98.2 kJ/mol. It is important
	to note that the chemical diffusion coefficient in strongly reduced
	conditions [p(O2) = 10-9 Pa] exhibits a negative temperature dependence
	of Dchem (-67.2 kJ/mol). This finding indicates that under these
	conditions transport in a chemical potential gradient is consistent
	with metallic charge transport.},
	Author = {Sheppard, L. R. and Bak, T. and Nowotny, J.},
	Doi = {10.1021/jp063703x},
	File = {Sheppard et al - J Phys Chem B 2006b.pdf:TiO2\\Nb doping\\Sheppard et al - J Phys Chem B 2006b.pdf:PDF},
	Journal = {J. Phys. Chem. B},
	Owner = {Ben},
	Pages = {22455--22461},
	Timestamp = {2008.06.09},
	Title = {Electrical Properties of Niobium-Doped Titanium Dioxide. 2. Equilibration Kinetics},
	Volume = {110},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/jp063703x}}

