	Journal = {Phys. Chem. Chem. Phys.},
	Owner = {Ben},
	Pages = {6128--},
	Timestamp = {2007.10.16},
	Title = {Global exploration of the energy landscape of solids on the \textit{ab initio} level},
	Volume = {9},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1039/b709943f}}

