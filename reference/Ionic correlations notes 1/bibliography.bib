	Owner = {Ben},
	Pages = {6061--6068},
	Timestamp = {2008.05.27},
	Title = {Light-Induced Charge Separation in Anatase TiO$_2$ Particles},
	Volume = {109},
	Year = {2005},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/jp0404293}}

