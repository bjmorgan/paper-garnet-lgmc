@article{Northrup_PhysB1999,
	Abstract = {A possible structure for inversion domain boundaries lying in the
	{1 1 Image 0} planes of wurtzite materials is presented. The formation
	energy of this defect is calculated for GaN using the local density
	approximation and first-principles pseudopotentials. The structure
	is generated by switching the chemical identity of Ga and N on one
	side of a (1 1 Image 0) plane followed by a translation Image. The
	resulting model, denoted IDB*-{1 1 Image 0}, contains no Ga--Ga or
	N--N bonds. The translation vector R generating IDB*-{1 1 Image 0}
	is the same as that generating the IDB*-{1 0 Image 0} model determined
	previously for the (1 0 Image 0) inversion domain boundary in GaN.
	Therefore, inversion domains extending along the c-axis may be enclosed
	by a mixture of IDB*-{1 0 Image 0} and IDB*-{1 1 Image 0} boundaries
	without the need for additional dislocations. The predicted formation
	energy (29 meV/{\AA}2) of the IDB*-{1 1 Image 0} boundary is greater
	by 4 meV/{\AA}2 than that of the IDB*-{1 0 Image 0} boundary. This higher
	energy is attributed to a greater density of fourfold rings on the
	(1 1 Image 0) boundary.},
	Author = {Northrup, John E.},
	Doi = {10.1016/S0921-4526(99)00423-8},
	File = {Northrup - Physica B 1999.pdf:Grain Boundaries\\Northrup - Physica B 1999.pdf:PDF},
