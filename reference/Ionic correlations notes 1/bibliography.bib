	Owner = {Ben},
	Pages = {9054--9063},
	Timestamp = {2007.11.27},
	Title = {Hydrogen Dissociation on Reconstructed {Z}n{O} Surfaces},
	Volume = {100},
	Year = {1996},
	Bdsk-Url-1 = {http://dx.doi.org/953704h}}

