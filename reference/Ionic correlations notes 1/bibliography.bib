	Owner = {Ben},
	Pages = {69--79},
	Timestamp = {2007.09.25},
	Title = {Theoretical analysis of the energy levels induced by oxygen vacancies and the doping process ({C}o, {C}u, {Z}n) on {S}n{O}$_2$(110) surface models},
