	Pages = {3704--3708},
	Timestamp = {2007.02.19},
	Title = {Localized and delocalized vibrations on {T}i{O}$_2$(110) studied by high-resolution electron-energy-loss spectroscopy},
	Volume = {30},
	Year = {1984}}

