	Owner = {Ben},
	Pages = {4318--4329},
	Timestamp = {2007.08.09},
	Title = {Nature of the Bottom $t_{2g}$-Block Bands of Layered Perovskites. Implications for the Transport Properties of Phases Where These Bands Are Partially Filled},
