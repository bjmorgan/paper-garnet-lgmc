@article{YagiEtAl_PhysRevB1996,
	Abstract = {Based on electrical resistivity and Hall-coefficient measurements
	from 2 to 370 K on slightly reduced semiconductive rutile single
	crystals with various oxygen deficiencies Od from 3.7×1018 to 1.3×1019/cm3,
	electronic conduction above 4 K is discussed in terms of two conduction
	bands separated by 0.03--0.05 eV. In this range of Od, defects are
