	Journal = {J. Phys. Chem. C},
	Owner = {Ben},
	Pages = {2624--2631},
	Timestamp = {2008.03.19},
	Title = {N Doping of Rutile {T}i{O}$_2$ (110) Surface. A Theoretical DFT Study},
	Volume = {112},
	Year = {2008},
