@article{HardmanEtAl_PhysRevB1999,
	Abstract = {Full-hemispherical x-ray photoelectron (Ti2p/O1s) and x-ray stimulated
	Auger electron (TiL3M23M23/O KVV) intensity distributions have been
	measured from TiO2(100)1×1 at relatively high-angular resolution
	($\pm$1.8$\,^{\circ}$). The results are compared with theoretical calculations using
