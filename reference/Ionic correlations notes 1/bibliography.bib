@article{LeeEtAl_CompMaterSci2004,
	Abstract = {The surface electronic structure of rutile TiO2 used for a photo-catalyst
	and an electrode of the dye-sensitized solar cell, was calculated
	by the DV-Xα method. The electrical properties of mesoporous TiO2
	electrode are dependent on the surface levels introduced by a high
