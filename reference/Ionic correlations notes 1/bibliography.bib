	Journal = {J. Chem. Phys.},
	Local-Url = {file://localhost/Users/ben/Documents/Papers/2000/Zimmer/Zimmer_J%20Chem%20Phys2000.pdf},
	Month = {Jan},
	Number = {14},
	Pages = {6416--6423},
	Pmid = {000086231600037},
	Rating = {0},
	Read = {Yes},
	Title = {Charge carrier interactions in ionic conductors: A classical molecular-dynamics and Monte Carlo study on {A}g{I}},
	Uri = {papers://FD885725-DF01-453D-8897-F9B3934FE7C8/Paper/p4803},
