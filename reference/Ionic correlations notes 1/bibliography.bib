	Journal = {Solid State Ionics},
	Keywords = {Mesoscopic Effect, Space-Charge Regions, Ion Conduction, Agi, Solid Electrolytes, Interfacial Phase Transition, Agi : Al2O3, Heterostructure},
	Local-Url = {file://localhost/Users/ben/Documents/Papers/2000/Lee/Lee_Solid%20State%20Ionics2000.pdf},
	Month = {Jan},
	Pages = {1261--1266},
	Pmid = {000165996600186},
	Rating = {0},
	Read = {Yes},
	Title = {A mesoscopic heterostructure as the origin of the extreme ionic conductivity in {A}g{I}: {A}l$_2${O}$_3$},
