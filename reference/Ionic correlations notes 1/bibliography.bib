	Title = {Periodic ab initio study of the hydrogenated rutile {T}i{O}$_2$(110) surface},
	Volume = {497},
	Year = {2002},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/S0039-6028(01)01477-7}}

