	Local-Url = {file://localhost/Users/ben/Documents/Papers/2000/Lee/Lee_J%20Electrochem%20Soc2000.pdf},
	Month = {Jan},
	Number = {6},
	Pages = {2407--2418},
	Pmid = {000087561800069},
	Rating = {0},
	Read = {Yes},
	Title = {Transport and phase transition characteristics in AgI : Al2O3 composite electrolytes - Evidence for a highly conducting 7-layer AgI polytype},
