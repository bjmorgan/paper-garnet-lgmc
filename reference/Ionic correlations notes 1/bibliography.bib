@article{RamamoorthyEtAl_PhysRevB1994A,
	Abstract = {We present self-consistent ab initio total-energy calculations on
	TiO2 (110) surfaces for which the atomic geometry has been relaxed
	to equilibrium. For stoichiometric supercells we find no surface
	states in the bulk band gap, in accord with experiment. Oxygen vacancies
	on the surface give rise to Ti 3d-like surface states at the edge
