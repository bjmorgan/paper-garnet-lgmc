	Owner = {Ben},
	Pages = {6126--6131},
	Timestamp = {2007.11.23},
	Title = {A Thermodynamically Stable Nanophase Material},
	Volume = {128},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/ja057121p}}

