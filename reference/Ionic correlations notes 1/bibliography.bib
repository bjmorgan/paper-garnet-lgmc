	Owner = {Ben},
	Pages = {7985--7991},
	Timestamp = {2007.10.03},
	Title = {Surface Structure of $(10\bar{1}0)$ and $(11\bar{2}0)$ Surfaces of {Z}n{O} with Density Functional Theory and Atomistic Simulation},
	Volume = {110},
	Year = {2006},
