@article{KodiyalamEtAl_PhysRevLett2004,
	Abstract = {Pressure-induced structural transformations in spherical and faceted
	gallium arsenide nanocrystals of various shapes and sizes are investigated
	with a parallel molecular-dynamics approach. The results show that
	the pressure for zinc blende to rocksalt structural transformation
	depends on the nanocrystal size, and all nanocrystals undergo nonuniform
	deformation during the transformation. Spherical nanocrystals above
	a critical diameter $\ge44$ \AA transform with grain boundaries.
	Faceted nanocrystals of comparable size have grain boundaries in
	60% of the cases, whereas the other 40% are free of grain boundaries.
	The structure of transformed nanocrystals shows that domain orientation
	and strain relative to the initial zinc blende lattice are not equivalent.
	These observations may have implications in interpreting the experimental
	x-ray line shapes from transformed nanocrystals.},
	Author = {Kodiyalam, Sanjay and Kalia, Rajiv K. and Nakano, Aiichiro and Vashishta, Priya},
	File = {Kodiyalam et al - Physical Review Letters 2004.pdf:C\:\\Documents and Settings\\Ben\\My Documents\\Papers\\Nanocrystals\\Kodiyalam et al - Physical Review Letters 2004.pdf:PDF},
	Journal = {Phys. Rev. Lett.},
	Number = {20},
