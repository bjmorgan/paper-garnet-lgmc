	Owner = {Ben},
	Pages = {7368--7375},
	Timestamp = {2008.05.08},
	Title = {Revealing the Nature of Trapping Sites in Nanocrystalline Titanium Dioxide by Selective Surface Modification},
	Volume = {107},
	Year = {2003},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/jp034064i}}

