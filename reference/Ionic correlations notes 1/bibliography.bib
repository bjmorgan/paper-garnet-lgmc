@article{Wang_JPhysCondensMatt2004,
	Abstract = {Zinc oxide is a unique material that exhibits semiconducting and piezoelectric
	dual properties. Using a solid-vapour phase thermal sublimation technique,
	nanocombs, nanorings, nanohelixes/nanosprings, nanobelts, nanowires
	and nanocages of ZnO have been synthesized under specific growth
