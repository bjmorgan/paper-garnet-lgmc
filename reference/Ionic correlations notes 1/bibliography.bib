	Volume = {10},
	Year = {2008},
	Bdsk-Url-1 = {http://dx.doi.org/10.1039/b719098k}}

@article{KoverEtAl_SurfIntAnal1995,
	Abstract = {High-resolution core and valence band photoemission as well as photoinduced
