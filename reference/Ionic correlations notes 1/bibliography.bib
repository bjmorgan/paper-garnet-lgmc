	Owner = {Ben},
	Pages = {11329--11335},
	Timestamp = {2007.01.08},
	Title = {Pressure-induced structural transformations in a medium-sized solicon nanocrystal by tight binding molecular dynamics},
	Volume = {117},
	Year = {2002}}

