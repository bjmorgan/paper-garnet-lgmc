@article{DengEtAl_PhysRevB2009,
	Abstract = {We introduce in detail our newly developed ab initio LDA+Gutzwiller
	(local density approximation plus Gutzwiller) method, in which the
	Gutzwiller variational approach is naturally incorporated with the
	density functional theory (DFT) through the "Gutzwiller density functional
	theory (GDFT)" (which is a generalization of original Kohn-Sham formalism).
	This method can be used for ground state determination of electron
	systems ranging from weakly correlated metal to strongly correlated
	insulators with long-range ordering. We will show that its quality
	for ground state is as high as that by dynamic mean field theory
	(DMFT), and yet it is computationally much cheaper. In additions,
	the method is fully variational, the charge-density self-consistency
	can be naturally achieved, and the quantities, such as total energy,
	linear response, can be accurately obtained similar to LDA-type calculations.
	Applications on several typical systems are presented, and the characteristic
	aspects of this new method are clarified. The obtained results using
	LDA+Gutzwiller are in better agreement with existing experiments,
	suggesting significant improvements over LDA or LDA+U.},
	Author = {Deng, Xiao Yu and Wang, Lei and Dai, Xi and Fang, Zhong},
	Doi = {10.1103/PhysRevB.79.075114},
	File = {:DFT\\Deng et al - Phys Rev B 2009.pdf:PDF},
	Journal = {Phys. Rev. B},
	Owner = {Ben},
	Pages = {075114},
	Timestamp = {2009.02.05},
	Title = {Local density approximation combined with Gutzwiller method for correlated electron systems: Formalism and applications},
	Volume = {79},
	Year = {2009},
	Bdsk-Url-1 = {http://dx.doi.org/10.1103/PhysRevB.79.075114}}

