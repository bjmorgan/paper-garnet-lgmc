	Title = {The Role of Interstitial Sites in the {T}i $3d$ Defect State in the Band Gap of Titania},
	Volume = {320},
	Year = {2008},
	Bdsk-Url-1 = {http://dx.doi.org/10.1126/science.1159846}}

@article{WestermarkEtAl_ChemPhys2002,
	Abstract = {In this work, photoelectron spectroscopy is used to measure the electronic
	density of states in the band gap region of a nanostructured TiO2
