	Title = {Peierls Distortions in Two and Three Dimensions and the Structures of {AB} Solids},
	Volume = {105},
	Year = {1983}}

@article{BurdettAndMcLarnan_JChemPhys1981,
	Abstract = {The structures of arsenic and black phosphorus can be regarded as
	distortions of the simple cubic or NaCl structure. The presence of
	more than four electrons per site results in the breaking of three
	of the six linkages around each atom in the NaCl structure. A generalization
