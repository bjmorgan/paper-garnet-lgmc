@article{ThomasAndMarks_PhysRevB2007,
	Abstract = {In a recent article [Phys. Rev. B 73, 165406 (2006)], Hallil et al.
	presented two empirical variable-charge models for TiO2. Both models
	are based on the QEq charge equilibration scheme, which we have recently
	shown [B. S. Thomas et al., Phys. Rev. B 74, 214109 (2004)] is inappropriate
	for this system due to charge shielding and the subsequent suppression
	of electrostatic forces. It is therefore questionable whether the
	extensions proposed by Hallil et al. provide transferability beyond
	that available with conventional partial charge models. We recommend
	that Hallil et al. test their method on other TixOy systems to evaluate
	its utility.},
