	Volume = {243},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/pssb.200666802}}

@article{RockerEtAl_PhysRevB1984,
	Abstract = {Electronic as well as localized and delocalized vibrational losses
	were studied on TiO$_2$(110) by electron-energy-loss spectroscopy.
	Electronic loss features at defect surfaces around $0.8\,\mathrm{eV}$
	indicate band-gap states. In the higher loss-energy range above $3\,\mathrm{eV}$
	well-known interband transitions were detected, which were found
	to be sensitive to ion bombardment, but not to the formation of point
	defects produced by high-temperature pretreatment. Vibrionic loss
	structures are characterized by three different delocalized Fuchs-Kliewer
	surface phonons at 46, 54, and 95 meV as well as multiple and combination
	losses thereof. Ion bombardment as well as high-temperature pretreatment
	reduces their intensities significantly. On defect-free surfaces
	a shift of all three fundamental frequencies or +2 meV relative to
	defect surfaces was observed for primary energies below 10 eV. Localized
	O--H stretching vibrations at $455\plusminus1\,\mathrm{meV}$ were
	detected on stoichiometric and defect surfaces after exposure to
	atomic hydrogen.},
	Author = {Rocker, G. and Schaefer, J. A. and G{\"{o}}pel, W.},
	File = {Gopel et al - Physical Review B 1984.pdf:C\:\\Documents and Settings\\Ben\\My Documents\\Papers\\TiO2\\Gopel et al - Physical Review B 1984.pdf:PDF},
	Journal = {Phys. Rev. B},
