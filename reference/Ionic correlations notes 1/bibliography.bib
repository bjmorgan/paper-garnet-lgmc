	Journal = {Surf. Sci.},
	Owner = {Ben},
	Pages = {226--245},
	Timestamp = {2007.02.19},
	Title = {Oxygen vacancies on {T}i{O}$_2$(110) and their interaction with {H}$_2${O} and {O}$_2$: A combined high-resolution {STM} and {DFT} study},
